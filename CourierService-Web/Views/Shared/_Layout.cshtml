<body>


    <!-- ======= Header ======= -->
    <header id="header" class="header d-flex align-items-center fixed-top">
        <div class="container-fluid container-xl d-flex align-items-center justify-content-between">



            <a asp-action="Index" asp-controller="Home" class="logo d-flex align-items-center">
                <!-- Uncomment the line below if you also wish to use an image logo -->
                <img src="~/home/assets/img/3.png" alt="">
                <img src="~/home/assets/img/text.png" alt="">

                @* <span class="secImg text-white font-weight-bold">
                <span class="imgDiv">

                </span>
                </span> *@
            </a>

            <i class="mobile-nav-toggle mobile-nav-show bi bi-list"></i>
            <i class="mobile-nav-toggle mobile-nav-hide d-none bi bi-x"></i>
            <nav id="navbar" class="navbar">
                <ul>
                    <li><a asp-action="Index" class="active">Home</a></li>
                    <li><a href="#About">About</a></li>
                    <li><a href="#Service">Services</a></li>
                    <li><a href="#Pricing">Pricing</a></li>

                    <li><a href="#Contact">Contact</a></li>
                    @if (RiderId != null)
                    {
                        <li><a asp-action="Index" asp-controller="Rider">@RiderEmail</a></li>
                        <li><a asp-action="Index" asp-controller="Rider">Dashboard</a></li>
                        <li><a asp-action="Logout" asp-controller="Home">Logout</a></li>
                    }
                    else if (AdminId != null)
                    {
                        <li><a class="font-weight-bold" asp-action="Index" asp-controller="Admin">@AdminEmail</a></li>
                        <li><a asp-action="Index" asp-controller="Admin">Dashboard</a></li>
                        <li><a asp-action="Logout" asp-controller="Home">Logout</a></li>
                    }
                    else if (MerchantId != null)
                    {
                        <li><a class="font-weight-bold" asp-action="Index" asp-controller="Merchant">@MerchantEmail</a></li>
                        <li><a asp-action="Index" asp-controller="Merchant">Dashboard</a></li>
                        <li><a asp-action="Logout" asp-controller="Home">Logout</a></li>
                    }
                    else
                    {
                        <li><a asp-action="Login">Login</a></li>
                        <li><a class="get-a-quote" asp-action="RegisterMerchant">Become A Merchant</a></li>

                    }


                </ul>
            </nav><!-- .navbar -->

        </div>
    </header><!-- End Header -->
    <!-- End Header -->



    <div class="">
        <main role="main" class="">
            <partial name="_Notification" />
            @RenderBody()
        </main>
    </div>


    <!-- ======= Footer ======= -->
    <footer id="footer" class="footer">

        <div class="container">
            <div class="row gy-4">
